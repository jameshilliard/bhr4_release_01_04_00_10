msgstr ""
"   -auth_scheme 1/2/3\n"
"      - método de autentificación para HTTP:\n"
"        1) Usuario, 2) Básico, 3) Abreviado (2 por defecto)\n"

#: options.h:1232
msgid "\t-auth_name $str    - name for Authorization (only via HTTP)\n"
msgstr ""
"   -auth_name $str\n"
"      - nombre para la Autentificación HTTP\n"

#: options.h:1243
msgid "\t-auth_passwd $str  - password for Authorization (only via HTTP)\n"
msgstr ""
"   -auth_passwd $str\n"
"      - contraseña para la Autentificación HTTP\n"

#: options.h:1254
msgid ""
"\t-auth_reuse_nonce/-noauth_reuse_nonce\n"
